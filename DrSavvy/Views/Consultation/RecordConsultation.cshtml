@section Scripts{
    <!--Scripts-->
    <script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.10.0.min.js" type="text/javascript">

    </script>
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.ui/1.9.2/jquery-ui.min.js" type="text/javascript">

    </script>
    <script type="text/javascript" src="https://ajax.cdnjs.com/ajax/libs/json2/20110223/json2.js">

    </script>
    <link href="~/Content/themes/base/jquery-ui.css" rel="stylesheet" />

    <script type="text/javascript">
        $(function () {
            $("#searchInput").autocomplete({
                source: function (request, response) {
                    $.ajax({
                        url: '/Consultation/AutoComplete/',
                        data: "{ 'prefix': '" + request.term + "'}",
                        dataType: "json",
                        type: "POST",
                        contentType: "application/json; charset=utf-8",
                        success: function (data) {
                            response($.map(data, function (item) {
                                return item;
                            }))
                        },
                        error: function (response) {
                            alert(response.responseText);
                        },
                        failure: function (response) {
                            alert(response.responseText);
                        }
                    });
                },
                select: function (e, i) {
                    $("#ProCode").val(i.item.val);
                    $("#ProName").val(i.item.label);
                    return i;
                },
                minLength: 1
            });
        });
    </script>
    <!--Scripts -->  
    <script>
        //Code for clock
        var clock;
        $(document).ready(function () {
            clock = $('.clock').FlipClock({
            clockFace: 'TwentyFourHourClock'
            });
            // declare consultation ID varable 
            var consultationID = new Array();
            $("#idx2").hide();
            $("#idx3").hide();
            $("#idx1").hide();
            $('#btnAdd').click(function () {
                var name = $("#searchInput").val();
                var code = $("#ProCode").val();
                if (name.trim() == "" )
                {
                    Swal.fire('<span style="color:#f0ad4e">Hold on...', 'No Known Procedures Have Been Selected', 'warning');
                    $("#btnAdd").prop("disabled", false);
                    return;
                }
                if (code.trim() == "") {
                    Swal.fire('<span style="color:#f0ad4e">Hold on...', 'The entred procedure is not Correct', 'warning');
                    $("#btnAdd").prop("disabled", false);
                    return;
                }
                else
                {
                    var code = $("#ProCode").val();
                    var name = $("#ProName").val();
                    var markup = "<tr id='myTableRow'><td name='record'> " + code + "</td><td>" + name + "</td><td><button type='button' class='btn btn-outline-success' id='BtnAddProcedures' style='border-radius:15px'><i class='fas fa-check'></i> Save</button></td><td><button type='button' class='btn btn-outline-danger' style='border-radius:15px' id='btnRemove'><i class='fas fa-times'></i> Cancel</button></td></tr>";
                    $(".searched").append(markup);
                    document.getElementById('btnAdd').disabled = true;
                    document.getElementById('searchInput').disabled = true;
                    $("#idx3").show();
                }
            })
            $('body').on('click','#btnRemove', function () {
                $(this).closest('tr').remove();
                $("#btnAdd").prop("disabled", false); 
                document.getElementById('searchInput').disabled = false;
                $('#searchInput').val('');
                $("#idx3").hide();
                $('#ProCode').val('');
            });
            //Div box above
            $("#BtnStartConsultation").click(function () {
                var note = '.';
                var patID = $('#pId').val();
                var appID = $('#aId').val();
                $("#BtnStartConsultation").hide();

                $.ajax({
                    url: "/Consultation/CreateConsultation",
                    type: "POST",
                    dataType: "json",
                    contentType: "application/json; charset=utf-8",
                    data: JSON.stringify({
                        "Patient_ID": patID,
                        "Appointment_ID": appID,
                        "Comments": note
                    }),
                    success: function (data) {
                        consultationID.push(data);
                        Swal.fire(
                            '<span style="color:limegreen">Consultation Created Successfully!',
                            'Lets Begin!',
                            'success'
                        );
                        $("#idx1").toggle();
                        $('#cId').val(data);
                        $('#cId1').val(data);
                        $('#cId2').val(data);
                        $('#cId3').val(data);
                        $('#cId4').val(data);

                    },
                    error: function (xhr, ajaxOptions, thrownError) {
                        Swal.fire({
                            type: 'error',
                            title: 'Oops...',
                            text: 'Something went wrong!',
                            footer: '<a href><span style="color:dodgerblue">Why do I have this issue?</a>'
                        });
                    }
                });
            });
            //Toggle button above
            $('body').on('click', '#BtnAddProcedures', function () { 
                var consID = parseInt(consultationID[0]);
                var procid = new Array();
                var procNotes = new Array();              
                $('#tblProcedures tr:gt(0)').each(function () {
                    procid.push($('td:eq(0)', this).text());
                    procNotes.push($('td:eq(1)', this).text());
                });
                var ProcedureID = parseInt(procid[0]);
                var ProcedureNotes = procNotes[0];
                $.ajax({
                    url: '/Consultation/AddProcedures',
                    type: "POST",
                    dataType: "json",
                    contentType: "application/json; charset=utf-8",
                    data: JSON.stringify({ "Consultation_ID": consID, "procid": ProcedureID, "procNotes": ProcedureNotes }),
                    success: function (Added) {
                        Swal.fire(
                            '<span style="color:#32CD32">Procedure Saved Successfuly',
                            '',
                            'success'
                        );
                        $('#myTableRow').remove();
                        $("#idx3").hide();
                        $("#idx2").show();
                        $("#tblAdded").find("tbody").empty();
                        $.each(Added, function (i, item) {
                            var mar = "<tr><td> " + item.ProcedureNotes + "</td></tr>";
                            $(".Added").append(mar);
                         $("#btnAdd").prop("disabled", false); 
                         document.getElementById('searchInput').disabled = false;
                            $('#searchInput').val('');
                            $('#ProCode').val('');
                            $("#idx2").show();
                        });
                    },
                    error: function (xhr, ajaxOptions, thrownError) {
                        Swal.fire({
                            type: 'error',
                            title: 'Oops...',
                            text: 'Something went wrong!',
                            footer: '<a href><span style="color:dodgerblue">Why do I have this issue?</a>'
                        });
                        $('#btnAdd').attr('disabled', false);
                        document.getElementById('searchInput').disabled = false;
                        $('#searchInput').val('');
                        $("#idx3").hide();
                    }
                });
            });
            //Save Consultation Remarks to Consultations Table
            $("#btnSaveRemarks").click(function () {
               var comment = $('#Comment').val();
               var consID = parseInt(consultationID[0]);
               var Patient = $('#pName').val();
               if (comment.trim() == "") {
                        Swal.fire('<span style="color:#f0ad4e"> Hold on....',' No Comments Have Been Entered','warning');
                        return false;
             }
                $.ajax({
                    url: '/Consultation/SaveComment',
                    type: "POST",
                    data: { "Consultation_ID": consID , "Comment": comment },
                    success: function (status) {
                        console.log(status)
                        Swal.fire('<span style="color:limegreen"> Done !!!','Remarks Saved Successfully','success');
                    },
                        error: function (xhr, ajaxOptions, thrownError) {
                            Swal.fire({
                            type: 'error',
                            title: 'Oops...',
                            text: 'Something went wrong!',
                            footer: '<a href><span style="color:dodgerblue">Why do I have this issue?</a>'
                        });
                    }
                });
            });
            //End Consultattion
            $("#btnEndConsultation").click(function () {
                var consID = parseInt(consultationID[0]);
                $.ajax({
                    url: '/Consultation/EndConsultation',
                    type: "POST",
                    data: { "Consultation_ID": consID },
                    success: function (status) {
                        console.log(status)
                        Swal.fire({
                            position: 'top',
                            type: 'success',
                            title: '<span style="color:gray">Consultation Has Been Ended',
                            showConfirmButton: false,
                            timer: 2000
                        });
                    },
                    error: function (xhr, ajaxOptions, thrownError) {
                        Swal.fire({
                            type: 'error',
                            title: 'Oops...',
                            text: 'Something went wrong!',
                            footer: '<a href>Why do I have this issue?</a>'
                        });
                    }
                }).done(
                    setTimeout( function() 
                    {
                       window.history.go(-1)
                     }, 3000)
                );
            });      
        });

    </script>
    <!--Flip Clock -->
    <link rel="stylesheet" href="~/Content/compiled/flipclock.css">
    <script src="~/Scripts/flipclock.js"></script>
}
